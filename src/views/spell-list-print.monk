{% for spell of data %}
    <tr>
        <td class="spell-name">
            <strong>
                {{ spell.name }}
            </strong>
        </td>
        <td class="spell-level">
            {{ spell.level }}
        </td>
        <td class="spell-description">
            <p>
                {{ spell.description }}
            </p>
        </td>
    </tr>
{% endfor %}
